{% extends 'base2.html.twig' %}

{% block head %}
    <head>

        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css" integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf" crossorigin="anonymous">
    </head>
    {%  block title  %}
        <title>Dashboard - checkupAdmin </title>
    {% endblock %}
{% endblock %}

{% block sales %}
    <div class="col-xxl-4 col-md-6">
        <div class="card info-card sales-card">


            <div class="card-body">
                <h5 class="card-title">Medecins <span>| Today</span></h5>

                <div class="d-flex align-items-center">
                    <div class="card-icon rounded-circle d-flex align-items-center justify-content-center">
                        <i class="fa fa-user-md"></i>
                    </div>
                    <div class="ps-3">
                        <h6>{{ medecins }}</h6>
                        <span class="text-success small pt-1 fw-bold">{{ percentM|round  }}%</span> <span class="text-muted small pt-2 ps-1">percent</span>

                    </div>
                </div>
            </div>

        </div>
    </div>
{% endblock %}
{% block revenu %}
    <div class="card-body">
        <h5 class="card-title">Patients <span>|Today</span></h5>

        <div class="d-flex align-items-center">
            <div class="card-icon rounded-circle d-flex align-items-center justify-content-center">
                <i class="fas fa-user-injured"></i>
            </div>
            <div class="ps-3">
                <h6>{{ patient }}</h6>
                <span class="text-success small pt-1 fw-bold">{{ percentP|round }}%</span> <span class="text-muted small pt-2 ps-1">percent</span>

            </div>
        </div>
    </div>

    </div>
    </div>
{% endblock revenu %}
{% block customers %}
    <div class="col-xxl-4 col-xl-12">

        <div class="card info-card customers-card">



            <div class="card-body">
                <h5 class="card-title">Customers <span>| This Year</span></h5>

                <div class="d-flex align-items-center">
                    <div class="card-icon rounded-circle d-flex align-items-center justify-content-center">
                        <i class="bi bi-people"></i>
                    </div>
                    <div class="ps-3">
                        <h6>{{ customers }}</h6>
                        <span class="text-danger small pt-1 fw-bold">12%</span> <span class="text-muted small pt-2 ps-1">percent</span>

                    </div>
                </div>

            </div>
        </div>

    </div><!-- End Customers Card -->
{% endblock %}
{% block chart %}


    <div class="card-body">
        <h5 class="card-title">Reports <span>/Today</span></h5>

    </div>

{% endblock %}
{% block Activity %}
{% endblock %}
{% block budget %}
{% endblock %}
