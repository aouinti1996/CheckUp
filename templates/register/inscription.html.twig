                    {{ form_start(form,{'attr':{'novalidate':'novalidate'}}) }}



                    <button type="button" class="btn btn-success btn-sm" data-dismiss="modal" onclick="do_save()">Save
                    </button>


                    {{ form_end(form) }}
                    <form>
